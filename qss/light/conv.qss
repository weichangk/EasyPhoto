QWidget#ConvView {
    background-color: "#1a1627";
    border-radius: 0px;
}

QWidget#ConvView_DividingLine {
    background-color: "#272336";
}

QCheckBox#ConvView_m_pSelectAllCkb {
    font-size: 13px;
    color: "#c4c4cc";
}

QPushButton#ConvView_m_pStartAllBtn {
    background-color: "#a070ff";
    color: "#ffffff";
    border-radius: 16px;
    font-size: 14px;
}

QPushButton#ConvView_m_pStartAllBtn:hover {
    background-color: "#ad85ff";
}

QPushButton#ConvView_m_pStartAllBtn:pressed {
    background-color: "#bb99ff";
}

QLabel#ConvView_m_pOutputFormatLbl,
QLabel#ConvView_m_pOutputFolderLbl {
    color: "#ffffff";
    font-size: 13px;
}

QLabel#ConvView_m_pTitleLbl {
    color: "#ffffff";
    font-size: 20px;
    font-weight: 600;
}

QWidget#ConvView_m_pListViewColumnName {
    background-color: "#1a1627";
}